<%= form_with url: path, method: method, local: true do |f| %>
<fieldset>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :breed %>
  <%= f.text_field :breed %>

  <%= f.label :age %>
  <%= f.number_field :age %>

  <%= f.label :adoptable %>
  <%= f.check_box adopt %>

  <%= f.submit %>
</fieldset>
<% end %>
